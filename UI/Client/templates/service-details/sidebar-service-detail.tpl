{if $sidebar['Service Details Actions']|@count}
  <div class="v-flex v-flex-col v-h-fit v-min-w-[260px] v-bg-white v-border v-border-[#EBEBF0] v-rounded v-shadow-card v-items-center md:v-ml-[30px]">
    <div class="v-flex v-flex-col v-w-full v-p-3">
      {foreach $sidebar as $item}
        {if $item->getName() == 'Service Details Actions'}
          <div menuItemName="{$item->getName()}" class="{if $item->getClass()} {$item->getClass()}{/if}" {if $item->getAttribute('id')} id="{$item->getAttribute('id')}" {/if}>
            <div class="v-text-lg v-font-medium v-text-gray-1 v-pl-2">
              {$item->getLabel()}
            </div>
            <div class="v-flex v-flex-col">
              {if $item->hasBodyHtml()}
                <div class="card-body">
                  {$item->getBodyHtml()}
                </div>
              {/if}
              {if $item->hasChildren()}
                <div class="v-w-full v-flex v-flex-col {if $item->getChildrenAttribute('class')} {$item->getChildrenAttribute('class')}{/if}" role="tablist">
                  {foreach $item->getChildren() as $childItem}
                    {if $childItem->getUri()}
                      <a menuItemName="{$childItem->getName()}" href="{$childItem->getUri()}"
                        class="v-flex v-flex-row v-mt-[5px] v-px-3 v-py-4 v-text-gray-1 hover:v-bg-[#ECF2F9] v-rounded-[4px] vnx-list-group-item {if $childItem->isDisabled()} v-pointer-events-none{/if}{if $childItem->getClass()} {$childItem->getClass()}{/if}{if $childItem->isCurrent()} active{/if}"
                        {if $childItem->getAttribute('dataToggleTab')} data-toggle="tab" {/if} {if $childItem->getAttribute('dataCustomAction')} {assign "customActionData" $childItem->getAttribute('dataCustomAction')} data-active="{$customActionData['active']}"
                        data-identifier="{$customActionData['identifier']}" data-serviceid="{$customActionData['serviceid']}" {/if} {if $childItem->getAttribute('target')} target="{$childItem->getAttribute('target')}" {/if} id="{$childItem->getId()}">

                        {if $childItem->hasBadge()}<span class="badge float-right">{$childItem->getBadge()}</span>{/if}
                        {if $childItem->hasIcon()}<i class="{$childItem->getIcon()}"></i>&nbsp;{/if}
                
                        {if $childItem->getName() == "Change Password"}
                          <img class="v-mr-2"
                            src="data:image/svg+xml;base64," />

                        {elseif $childItem->getName() == "cpanel"}
                          <img class="v-mr-2"
                            src="data:image/svg+xml;base64," />

                        {elseif $childItem->getName() == "webmail"}
                          <img class="v-mr-2"
                            src="data:image/svg+xml;base64," />

                        {elseif $childItem->getName() == "Custom Module Button Cấu hình tên miền"}
                          <img class="v-mr-2"
                            src="data:image/svg+xml;base64," />
                        {elseif $childItem->getName() == "Custom Module Button Xem Logs chi tiết"}
                          <img class="v-mr-2"
                            src="data:image/svg+xml;base64," />
                        
                        {elseif $childItem->getName() == "Custom Module Button Xem Graph chi tiết"}
                          <img class="v-mr-2"
                            src="data:image/svg+xml;base64," />
                        
                        {elseif $childItem->getName() == "Custom Module Button Nâng cấp lưu lượng"}
                          <img class="v-mr-2"
                            src="data:image/svg+xml;base64," />
                        
                        {elseif $childItem->getName() == "Upgrade/Downgrade"}
                          <img class="v-mr-2"
                            src="data:image/svg+xml;base64," />
    
                        {else}
                          <img class="v-mr-2"
                            src="data:image/svg+xml;base64," />
                        {/if}

                        {$childItem->getLabel()}
                        <div class="v-flex-1 v-min-w-[10px]"></div>

                        {if isset($customActionData)}
                          <span class="loading hidden w-hidden v-mr-2" style="display: none;"><i class="fas fa-spinner fa-spin"></i></span>
                        {/if}
                        
                        <div class="v-center"><i class="fas fa-chevron-right card-minimise float-right v-text-gray-4"></i></div>
                      </a>
                    {else}
                      <div menuItemName="{$childItem->getName()}" class="{if $childItem->getClass()} {$childItem->getClass()}{/if}" id="{$childItem->getId()}">
                        {if $childItem->hasBadge()}<span class="badge float-right">{$childItem->getBadge()}</span>{/if}
                        {if $childItem->hasIcon()}<i class="{$childItem->getIcon()}"></i>&nbsp;{/if}
                        {$childItem->getLabel()}
                      </div>
                    {/if}
                  {/foreach}
                </div>
              {/if}
            </div>
            {if $item->hasFooterHtml()}
              <div class="card-footer clearfix">
                {$item->getFooterHtml()}
              </div>
            {/if}
          </div>
        {/if}
      {/foreach}
    </div>
  </div>
{/if}
<script>
  window.addEventListener("DOMContentLoaded", (event) => {
    document.querySelectorAll('.vnx-list-group-item[href^="#"]').forEach(function(node) {
      node.addEventListener("click", function() {
        document.querySelectorAll('.vnx-list-group-item[href^="#"]').forEach(function(node2) {
          jQuery(node2).removeClass('active')
        });
      });
    });
  });
</script>